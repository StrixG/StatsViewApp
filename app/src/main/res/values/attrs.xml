<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatsView">
        <attr name="lineWidth" format="dimension|reference" />
        <attr name="fontSize" format="dimension|reference" />
        <attr name="colors" format="reference" />
        <attr name="android:progressBackgroundTint" format="color" />
        <attr name="animationType" format="enum">
            <enum name="parallel" value="0"/>
            <enum name="parallel_rotation" value="1"/>
            <enum name="sequential" value="2"/>
        </attr>
    </declare-styleable>
</resources>
